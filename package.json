{
  "name": "monument-pwa",
  "description": "Monuments PWA for Yatra",
  "author": "Yatra Online Pvt Ltd",
  "private": true,
  "scripts": {
    "dev": "node server",
    "prod": "cross-env NODE_ENV=production && npm run forev",
    "start": "cross-env NODE_ENV=production node server",
    "build": "rimraf monuments && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
    "forev": "cross-env NODE_ENV=production forever  start server.js && forever list",
    "list": "forever list",
    "stop": "forever stopall",
    "logs": "forever logs"
  },
  "engines": {
    "node": ">=7.0",
    "npm": ">=4.0"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "cross-env": "^5.1.4",
    "es6-promise": "^4.2.4",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "forever": "^0.15.3",
    "lru-cache": "^4.1.2",
    "morgan": "^1.9.0",
    "node-sass": "^4.7.2",
    "qs": "^6.5.1",
    "route-cache": "0.4.3",
    "sass-loader": "^6.0.6",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.16",
    "vuejs-datepicker": "^0.9.29",
    "vuetify": "0.17.6",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.6",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-imports": "^1.5.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "chokidar": "^1.7.0",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "rimraf": "^2.6.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "sw-precache-webpack-plugin": "^0.11.5",
    "url-loader": "^0.6.2",
    "vue-loader": "^13.7.1",
    "vue-style-loader": "^3.1.2",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^3.11.0",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-hot-middleware": "^2.21.2",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.6.0"
  }
}
